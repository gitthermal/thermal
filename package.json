    "@babel/core": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "ajv": "^6.10.2",
    "apexcharts": "^3.9.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.4.5",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "5.0.4",
